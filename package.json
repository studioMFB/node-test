  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studioMFB/node-test.git"
  },
  "bugs": {
    "url": "https://github.com/studioMFB/node-test/issues"
  },
  "homepage": "https://github.com/studioMFB/node-test#readme",
  "type": "module",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0"
  }
