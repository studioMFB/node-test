  "license": "ISC",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev-s": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server/main.ts",
    "dev-c": "ts-node-esm ./src/client/main.ts",
