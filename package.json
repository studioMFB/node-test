{
  "name": "node-test",
  "version": "1.0.0",
  "description": "To play about with Node.js",
  "main": "./src/client/main.ts",
  "author": "Francois Brunet",
  "license": "ISC",
  "scripts": {
    "clean": "rimraf ./dist/",
    "copy-assets": "ts-node ./src/tools/copyAssets",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "tsc": "tsc",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build-ts": "npm-run-all clean lint tsc copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts",
    "start": "node .",
    "prestart": "npm run build",
    "dev-s": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server/main.ts",
    "dev-c": "vite",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studioMFB/node-test.git"
  },
  "bugs": {
    "url": "https://github.com/studioMFB/node-test/issues"
  },
  "homepage": "https://github.com/studioMFB/node-test#readme",
  "type": "module",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.3.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.9.2",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "shelljs": "^0.8.5",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
